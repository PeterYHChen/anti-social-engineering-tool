{
    "manifest_version": 2,
    "name": "Social Engineering Defender",
    "description": "This extension allows to user to observe social engineering properties of sites if they exist.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "ASET"
    },
    "permissions": [
        "https://anti-social-engineering-tool.mybluemix.net/*",
        "https://safebrowsing.googleapis.com/*",
        "https://api.mywot.com/*",
        "activeTab",
        "storage"
    ]
}